package Oops;
public class HasRelationship {
	public static void main(String[] args) {
	 A3 a =new A3();
	 A2 b= new A2(a);
	 A1 c = new A1(b);
	 c.show();
	}
}
class A1{
	String name = "sree";
	int  x = 50;
	A2 z;
	A1(A2 z){
		this.z= z;
	}
	
	void show() {
		System.out.println(this.name+"\t"+this.x+"\t"+z.y);
	}
}
class A2{
	int y = 22;
	A3 b;
	A2(A3 b){
		this.b= b;
		System.out.println(b.lastname);
}
}
class A3{
String lastname="Bodas";
}