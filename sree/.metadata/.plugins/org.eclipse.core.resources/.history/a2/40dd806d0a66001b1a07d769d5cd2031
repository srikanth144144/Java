package Oops.Test;
class Bank{
	 private String name;
	 Bank(String name){
		 this.name=name;
	 }
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}	
}
class Employee{
	String Employee_name;
	Employee(String Employee_name){
		this.Employee_name = Employee_name;
	}
	public String getEmployee_name() {
		return Employee_name;
	}
	public void setEmployee_name(String employee_name) {
		Employee_name = employee_name;
	}	 
	 }

class Employee1{
	String Employee_name;
	Employee1(String Employee_name){
		this.Employee_name = Employee_name;
	}
	public String getEmployee_name() {
		return Employee_name;
	}
	public void setEmployee_name(String employee_name) {
		Employee_name = employee_name;
	}	 
	 }

public class Has {
	public static void main(String[] args) {
		Bank b =new Bank("SBI");
		Employee e =new Employee("Madhu");
		Employee1 e1 = new Employee1("Hari");
		System.out.println(e.getEmployee_name()+" "+e1.getEmployee_name()  +  "is employee of "+b.getName());
	}
}
