package Oops;

public class HasRelationship2 {

	public static void main(String[] args) {
	    Games a = new Games();
	    Play p = new Play();
	    p.setBatmenten(a);
	    p.show();
	}
}
class Games{
	void batmenten() {
		System.out.println(" There are two players in batmenten ");
		void kho() {
			System.out.println("There are 12 player");
			
		}
	}
}
class Play{
	private Games Batmenten;

	public Games getBatmenten() {
		return Batmenten;
	}

	public void setBatmenten(Games batmenten) {
		Batmenten = batmenten;
	}
	public void show() {
		Batmenten.batmenten();
	}
	
}
class Play1{
	private Games khoo;

	public Games getKhoo() {
		return khoo;
	}

	public void setKhoo(Games khoo) {
		this.khoo = khoo;
	}
	public void show1() {
		khoo.kho();
	}
	
}
